---
title: Default layout
is_layout: false
---
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>{{ page.title }}</title>

    <meta name="keywords" value="{{ site.meta_keywords }}">
    <meta name="description" value="{{ site.meta_description }}">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <script src="https://use.typekit.net/udt1gzh.js"></script>

    <script>
      try{Typekit.load({ async: true });}catch(e){}
    </script>

    <link rel="stylesheet" href="https://use.fontawesome.com/82de14177b.css">

    {{ 'main.css' | stylesheet_tag }}
    {{ 'vendor.bundle.js' | javascript_tag }}
    {%comment%}Google and Piwik Analytics{%endcomment%}
    {% include 'site-analytics' %}
  </head>

  <body class="body-{{ page.slug }}">
    {% include 'header' %}

    <main>
      {% block 'heading' %}
        {% include 'breadcrumbs-heading' %}
      {% endblock %}

      {% block 'main' %}
      {% endblock %}
    </main>

    {% include 'footer' %}
  </body>
</html>
